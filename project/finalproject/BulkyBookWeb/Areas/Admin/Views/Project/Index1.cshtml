<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Details List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Project" asp-action="Details" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> &nbsp; Create New Project
            </a>
        </div>
    </div>
    <br /><br />

 

    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Customer</th>
                <th>JobTitle</th>
                <th>SkillSet</th>
                <th>Experience</th>
                <th>Number Of Months</th>
                <th>Start Date</th>
                <th>End Sate</th>
                <th>Number Of Resources</th>

 

                <th></th>
            </tr>
        </thead>
    </table>
</div>

 

@section Scripts{
        <script src="~/js/details.js"></script>
}